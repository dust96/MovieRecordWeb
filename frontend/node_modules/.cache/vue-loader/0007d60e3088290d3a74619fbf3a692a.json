 <b-form-select\n          id=\"input-3\"\n          v-model=\"form.food\"\n          :options=\"foods\"\n          required\n        ></b-form-select>\n      </b-form-group>\n\n      <b-button type=\"submit\" variant=\"primary\">Submit</b-button>\n      <b-button type=\"reset\" variant=\"danger\">Reset</b-button>\n    </b-form>\n  </div>\n</template>\n\n<script>\n\n  export default {\n    name: \"AddGoods\",\n     data() {\n      return {\n        form: {\n          email: '',\n          name: '',\n          food: null\n        },\n        foods: [{ text: 'Select One', value: null }, 'Carrots', 'Beans', 'Tomatoes', 'Corn'],\n        show: true\n      }\n    },\n    methods: {\n      onSubmit(event) {\n        event.preventDefault()\n        alert(JSON.stringify(this.form))\n      },\n      onReset(event) {\n        event.preventDefault()\n        // Reset our form values\n        this.form.email = ''\n        this.form.name = ''\n        this.form.food = null\n        // Trick to reset/clear native browser form validation state\n        this.show = false\n        this.$nextTick(() => {\n          this.show = true\n        })\n      }\n    }\n  }\n   \n</script>\n\n<style>\n  \n\n</style>"]}]}